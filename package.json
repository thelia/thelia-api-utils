{
  "name": "@openstudio/thelia-api-utils",
  "homepage": "https://github.com/thelia/thelia-api-utils",
  "version": "0.1.0",
  "description": "collection of javascript function & React hooks to use Thelia OpenApi",
  "main": "es/index.js",
  "module": "es/index.js",
  "browser": "dist/TheliaAPI.umd.js",
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "rollup --watch --config",
    "build:umd": "rimraf ./dist && rollup --config",
    "build:es": "rimraf ./es && tsc",
    "build:types": "rimraf ./types && tsc --project tsconfig.types.json",
    "build": "npm run build:umd && npm run build:es && npm run build:types"
  },
  "keywords": [
    "thelia"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thelia/thelia-api-utils.git"
  },
  "author": "OpenStudio",
  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/thelia/thelia-api-utils/issues"
  },
  "files": [
    "dist",
    "es",
    "types"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/qs": "^6.9.7",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.0",
    "rollup-plugin-filesize": "^9.1.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "qs": "^6.10.1",
    "react-query": "^3.18.1"
  }
}
